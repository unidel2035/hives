{
  "name": "vue-polza-chat",
  "version": "1.0.0",
  "description": "Vue.js chat interface with Polza AI and file system tools support",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "server": "node server/index.js",
    "server:dev": "nodemon server/index.js",
    "start": "concurrently \"npm run dev\" \"npm run server:dev\""
  },
  "dependencies": {
    "vue": "^3.4.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.0",
    "@types/node": "^20.11.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vue-tsc": "^1.8.0",
    "concurrently": "^8.2.0",
    "nodemon": "^3.0.0"
  },
  "keywords": [
    "vue",
    "polza",
    "ai",
    "chat",
    "tools",
    "file-system"
  ],
  "author": "",
  "license": "MIT"
}
